import java.util.*;

class Solution {
    public ArrayList<ArrayList<Integer>> countFreq(int[] arr) {
        int n = arr.length;
        Arrays.sort(arr);
        ArrayList<ArrayList<Integer>> result = new ArrayList<>();
        int count = 1;
        
        for(int i = 0; i < n; i++) {
            if(i < n-1 && arr[i] == arr[i+1]) {
                count++;
            } else {
                ArrayList<Integer> pair = new ArrayList<>();
                pair.add(arr[i]);      // Element
                pair.add(count);       // Frequency
                result.add(pair);
                count = 1;
            }
        }
        
        return result;
    }
}
