class Solution {
    public void moveZeroes(int[] nums) {
        int n = nums.length;
        int nonZeroPos = 0;
        
        // Move non-zero elements to front
        for(int i = 0; i < n; i++) {
            if(nums[i] != 0) {
                nums[nonZeroPos] = nums[i];
                nonZeroPos++;
            }
        }
        
        // Fill remaining with zeros
        for(int i = nonZeroPos; i < n; i++) {
            nums[i] = 0;
        }
    }
}
